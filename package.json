{
  "name": "receipt-wrangler",
  "version": "1.0.0",
  "description": "SMS-based receipt categorization bot for family budgeting",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "concurrently -k -n server,client -c blue,magenta \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:server": "NODE_ENV=development tsx watch src/index.ts",
    "dev:client": "cd client && pnpm dev",
    "build": "pnpm build:client",
    "build:client": "cd client && pnpm install && pnpm build",
    "typecheck": "tsc --noEmit",
    "start": "NODE_ENV=production tsx src/index.ts",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md,html}": "prettier --write"
  },
  "keywords": [
    "twilio",
    "sms",
    "receipt",
    "budget"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.73",
    "ai": "6.0.0-beta.120",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "preact": "^10.27.2",
    "preact-render-to-string": "^6.6.3",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.20.6",
    "twilio": "^5.10.6",
    "zod": "^4.1.13"
  }
}
